%section.detail
  = render :partial => '/users/shared/user_sidebar', :locals => { :user => @user }
  %article.main
    %section.overview
      = render :partial => '/users/shared/overview_header'
      %section.overview-main
        %h3 Eigene Begegnungen
        = render(:partial => 'users/overview/course', :collection => @user.get_courses) || "keine"

        %h3 Andere Begegnungen
        = render(:partial => 'users/overview/course', :collection => @user.get_enquired_courses) || "keine"





